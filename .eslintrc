{
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
      "ecmaVersion": 5
  },
  "env": {
    "browser": true,
    "es6": false
  },
  "globals": {
    "$": true,
    "FB": true,
    "Materialize": true,
    "Chart": true
  },
  "rules": {
    "no-trailing-spaces": 0,
    "semi": [2, "always"],
    "no-eq-null": 2,
    "no-var": 0
  }
}